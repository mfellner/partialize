{
  "name": "@mfellner/partialize",
  "version": "0.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Maximilian Fellner <max.fellner@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/mfellner/partialize",
  "keywords": [
    "partial",
    "typescript",
    "maybe",
    "optional"
  ],
  "engines": {
    "node": ">=8.11.1"
  },
  "scripts": {
    "dist": "tsc -p .",
    "lint": "tslint --type-check --project tsconfig.json",
    "test": "npm run lint && jest --coverage",
    "prepublishOnly": "npm run dist"
  },
  "devDependencies": {
    "@types/jest": "22.2.2",
    "@types/node": "9.6.2",
    "jest": "22.4.3",
    "ts-jest": "22.4.2",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.8.1"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/test/__mocks__/file-mock.js",
      "\\.(css|scss|less)$": "<rootDir>/test/__mocks__/style-mock.js"
    },
    "transform": {
      "\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./test/tsconfig.json"
      }
    },
    "testRegex": ".*\\.test.ts$",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
